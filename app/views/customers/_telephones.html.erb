<table width="870" border="0" align="center" cellpadding="5" cellspacing="0" id='telephones_table'>
	<thead>
		<tr>
			<th width="16" scope="col">&nbsp;</th>
			<th width="16" scope="col">&nbsp;</th>
			<th width="240" scope="col">NÃºmero</th>
		</tr>
	</thead>
	<tbody>
		<% @telephones.each do |t| %>
			<tr class="item_list">
				<td width="16">
					<%= link_to image_tag("edit.png",size:"16x16"), '#', class: 'edit', id: 'edit_'+t.id.to_s %>
