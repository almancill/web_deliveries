<table width="870" border="0" align="center" cellpadding="5" cellspacing="0" id="news-list" class="pretty">
	<thead>
		<tr>
			<th width="16" scope="col">&nbsp;</th>
			<th width="16" scope="col">&nbsp;</th>
			<th width="240" scope="col">Número</th>
		</tr>
	</thead>
	<tbody>
		<% @telephones.each do |t| %>
			<tr class="item_list">
				<td width="16">
					<%= link_to image_tag("edit.png",size:"16x16"), '#' %>
				</td>
				<td width="16">
					<%= link_to image_tag("delete.png",size:"16x16"), t, method: :delete, confirm: "¿Estas seguro que desea eliminar este Número?"%>
				</td>
				<td width="240"><%= t.number %></td>
			</tr>
		<% end %>
	</tbody>
</table>