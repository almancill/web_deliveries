<div id="container">
	<%= render 'shared/admin_login_info' %>
	<div id="header">
		<%= render 'shared/admin_header' %>
	</div>
	<div id="navcontainer">
		<%= render 'shared/admin_menu', page: 1 %>
	</div>

	<div id="content">
		<% if flash[:notice]%>
			<p class="done"> <%= flash[:notice]%></p>
		<% end %>
		<%= form_tag('#') do %>
			<p><%= link_to 'Nuevo Cliente', new_customer_path, class: "button"%></p>
		<% end %>
		<div id="customers">
			<%= render 'customers' %>
		</div>
	</div>
</div>
