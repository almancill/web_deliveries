			<th width="16" scope="col">&nbsp;</th>
			<th width="240" scope="col">Direcci√≥n</th>
		</tr>
	</thead>
	<tbody>
		<% @addresses.each do |t| %>
			<tr class='item_list' >
				<td width="16">
					<%= link_to image_tag("edit_address.png",size:"16x16"), '#', class: 'edit-address', id: 'edit-address_'+t.id.to_s %>
